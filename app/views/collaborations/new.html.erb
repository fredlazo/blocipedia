<br>
<div class="row">
  <div class='col-md-4'></div>
  <div class='col-md-4'>
    <h4>Add Collaborator</h4>
      <%= form_for @collaboration, url: wiki_collaborations_path do |f| %>
        <div class="form-group">
          <%= f.collection_select :user_id, @wiki.available_users, :id, :username, :prompt => true %>
        </div>
        <div class="form-group">
          <%= f.submit "Save", class: 'btn btn-success' %>
        </div>

      <% end %>
    </div>
    <div class='col-md-4'></div>
</div>
